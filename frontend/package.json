{
  "name": "ck-missive-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "overrides": {
    "lodash": ">=4.17.21"
  },
  "scripts": {
    "dev": "vite",
    "dev:clean": "powershell -ExecutionPolicy Bypass -File ../scripts/port-manager.ps1 -Action kill -Service frontend && vite",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "port:check": "powershell -ExecutionPolicy Bypass -File ../scripts/port-manager.ps1 -Action check",
    "port:kill": "powershell -ExecutionPolicy Bypass -File ../scripts/port-manager.ps1 -Action kill -Service frontend",
    "prepare": "husky",
    "api:generate": "openapi-typescript http://localhost:8001/openapi.json -o src/types/generated/api.d.ts",
    "api:generate:file": "openapi-typescript ../docs/openapi.json -o src/types/generated/api.d.ts",
    "api:generate:changelog": "node scripts/type-changelog.js generate"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.1",
    "@ck-shared/core": "file:../../shared-modules/core",
    "@ck-shared/site-management-module": "file:../../shared-modules/site-management-module",
    "@ck-shared/ui-components": "file:../../shared-modules/ui-components",
    "@tanstack/react-query": "^5.45.1",
    "@tanstack/react-query-devtools": "^5.45.1",
    "@types/react-highlight-words": "^0.20.1",
    "antd": "^5.21.0",
    "axios": "^1.7.0",
    "dayjs": "^1.11.18",
    "google-auth-library": "^10.3.0",
    "jwt-decode": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-highlight-words": "^0.21.0",
    "react-router-dom": "^6.30.1",
    "recharts": "^3.2.0",
    "swagger-ui-react": "^5.29.0",
    "xlsx": "^0.18.5",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@playwright/test": "^1.58.1",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^14.2.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/lodash": "^4.17.21",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^9.1.7",
    "jsdom": "^24.0.0",
    "lint-staged": "^16.1.6",
    "msw": "^2.1.0",
    "openapi-typescript": "^7.10.1",
    "typescript": "^5.6.0",
    "vite": "^5.4.19",
    "vitest": "^1.2.0"
  }
}
